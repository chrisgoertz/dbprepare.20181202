            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label fx:id="lblDb1" text="Database 1" />
                  <Button fx:id="btnDb1" mnemonicParsing="false" text="update" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label fx:id="lblDb2" text="Database 2" />
                  <Button fx:id="btnDb2" mnemonicParsing="false" text="update" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
            <ScrollPane fitToHeight="true" fitToWidth="true">
               <content>
                  <TextArea fx:id="taLog" prefHeight="122.0" prefWidth="600.0" />
               </content>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
