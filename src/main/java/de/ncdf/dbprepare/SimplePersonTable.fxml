<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox focusTraversable="true" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.ncdf.dbprepare.PersonTableController">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="1000.0" prefWidth="1600.0">
         <content>
            <TableView fx:id="teilnehmerTable" tableMenuButtonVisible="true">
              <columns>
                <TableColumn fx:id="columnNachname" prefWidth="-1.0" text="Nachname" />
                <TableColumn fx:id="columnVorname" minWidth="0.0" prefWidth="197.0" text="Vorname" />
                  <TableColumn fx:id="columnStrasse" prefWidth="-1.0" text="Strasse" />
                  <TableColumn fx:id="columnWohnort" prefWidth="-1.0" text="Wohnort" />
                  <TableColumn fx:id="columnPostleitzahl" minWidth="100.0" prefWidth="77.0" text="Postleitzahl" />
                  <TableColumn fx:id="columnGeschlecht" minWidth="100.0" prefWidth="75.0" text="Geschlecht" />
                  <TableColumn fx:id="columnTelefonnummer" minWidth="150.0" prefWidth="75.0" text="Telefonnummer" />
                  <TableColumn fx:id="columnEmailadresse" minWidth="250.0" prefWidth="75.0" text="Emailadresse" />
                  <TableColumn fx:id="columnGeburtstag" minWidth="150.0" prefWidth="75.0" text="Geburtstag" />
                  <TableColumn fx:id="columnEintrittsdatum" minWidth="150.0" prefWidth="75.0" text="Eintrittsdatum" />
                  <TableColumn fx:id="columnRole" minWidth="100.0" prefWidth="75.0" text="Role" />
                  <TableColumn fx:id="columnRFID" prefWidth="75.0" text="RFID" />
                  <TableColumn fx:id="columnID" prefWidth="75.0" text="ID" />
              </columns>
            </TableView>
         </content>
      </ScrollPane>
   </children>
</VBox>
